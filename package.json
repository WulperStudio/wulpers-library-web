    "aekstrom-react-medium-editor": "^1.1.0",
    "antd": "^4.10.2",
    "clsx": "^1.1.1",
    "date-fns": "^2.16.1",
    "i": "^0.3.6",
    "jsonpath": "^1.0.2",
