    "npm": "^6.14.11",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-medium-editor": "^1.8.1",
    "react-scripts": "4.0.1",
    "react-sticky": "^6.0.3",
    "react-string-replace": "0.4.4"
  },
