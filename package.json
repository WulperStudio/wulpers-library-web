{
  "name": "@wulpers-ui/core",
  "private": false,
  "description": "wulpers-library-web",
  "version": "0.0.1-beta-12",
  "license": "0BSD",
  "main": "index.js",
  "author": "Wulpers Co.",
  "scripts": {
    "start": "start-storybook -p 9009 -s static",
    "build:storybook": "build-storybook -s static",
    "build:babel": "npm-run-all _clear-dist _build-components _cp-assets _cp-package",
    "_build-components": "babel ./src/ --ignore assets,stories --out-dir ./dist --extensions '.tsx'",
    "_cp-assets": "cp -R ./src/assets ./dist",
    "_cp-package": "cp ./package.json ./dist",
    "_clear-dist": "rm -rf dist && mkdirp ./dist",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "test": "echo \"Write tests!\" && exit 1"
  },
  "dependencies": {
    "@material-ui/core": "4.11.2",
    "@material-ui/icons": "4.11.2",
    "@material-ui/lab": "4.0.0-alpha.57",
